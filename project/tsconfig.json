  "compilerOptions": {
    "moduleResolution": "node",
    "jsx": "react-jsx",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "strict": true,
    "resolveJsonModule": true,
    "outDir": "dist",
    "module": "CommonJS",
    "target": "ES2020",
    "sourceMap": true,
    "allowJs": true
  },
  "include": [
    "src/**/*.ts",
    "**/*.ts",
    "src/**/*",
    "migrateUsers.ts",
  ],
  "exclude": [
    "node_modules",
    "dist"
  ]
}